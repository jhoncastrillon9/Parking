@model Parking.Models.CheckIngCheckOut

@{
    ViewData["Title"] = "Create";
}

<h1>Crear</h1>

<h4>Registro de Salidad</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateCheckOut">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="VehicleId" class="control-label"></label>
                <select asp-for="VehicleId" class ="form-control" asp-items="ViewBag.Vehicles"></select>
            </div>
            <br />
            <br />
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" />
                 <a  class="btn btn-secondary" asp-action="Index">Regresar</a>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
