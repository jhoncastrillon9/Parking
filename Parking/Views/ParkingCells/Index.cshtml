@model IEnumerable<Parking.Models.ParkingCell>

@{
    ViewData["Title"] = "Index";
}



<div style="display: flex; justify-content: space-between;">
  <div id="emptyDiv"><h1>Celdas</h1></div>
  <div> <a class="btn btn-primary"  asp-action="Create">Crear nuevo</a></div>
</div>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Note)
            </th>
            <th>Opciones</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Note)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ParkingCellId">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.ParkingCellId">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.ParkingCellId">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
